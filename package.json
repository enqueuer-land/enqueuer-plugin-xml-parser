{
  "name": "enqueuer-plugin-xml-parser",
  "version": "1.0.0",
  "description": "Enqueuer xml parser plugin",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf dist",
    "unitTest": "node_modules/.bin/jest",
    "all": "npm run clean && npm run lint && npm run build && npm run unitTest",
    "lint": "node_modules/.bin/tslint --project tsconfig.json",
    "prepublishOnly": "npm run all",
    "build": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/enqueuer-land/enqueuer-plugin-xml-parser.git"
  },
  "keywords": [
    "enqueuer",
    "nqr",
    "xml",
    "plugin"
  ],
  "author": "Virgs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/enqueuer-land/enqueuer-plugin-xml-parser/issues"
  },
  "homepage": "https://github.com/enqueuer-land/enqueuer-plugin-xml-parser#readme",
  "devDependencies": {
    "@types/jest": "^25.1.4",
    "@types/node": "^14.0.6",
    "jest": "^24.5.0",
    "ts-jest": "^26.0.0",
    "ts-node": "^8.0.2",
    "tslint": "^5.11.0",
    "typescript": "^3.0.1"
  },
  "dependencies": {
    "enqueuer": "^5.1.1",
    "pixl-xml": "^1.0.13"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "statements": 95,
        "branches": 95,
        "functions": 95,
        "lines": 95
      }
    },
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": ".*\\.test\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  }
}
